package it.prisma.domain.dsl.paas.services.caaas;

public class CaException extends Exception {

    private static final long serialVersionUID = 1L;

    public CaException(String message) {
	super(message);
    }

    public CaException(Throwable e) {
	super(e);
    }

    public CaException(String message, Throwable e) {
	super(message, e);
    }

}
