package it.prisma.domain.dsl.accounting;

public enum RoleEnum {

    PRISMA_USER("PRISMA_USER"), PRISMA_ADMIN("PRISMA_ADMIN"), PRISMA_WEBUI("PRISMA_WEBUI"), PRISMA_MONITORING(
	    "PRISMA_MONITORING");

    private final String role;

    private RoleEnum(final String role) {
	this.role = role;
    }

    public String getRoleString() {
	return this.role;
    }

}